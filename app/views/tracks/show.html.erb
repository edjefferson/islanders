<h2><%= link_to @track.artist.name, @track.artist %> - <%= @track.track %></h2>




<table class = "table table-striped table-sm">
  <tr>
    <th>Chosen by</th>
    <th>Chosen on</th>
  </tr>
<% @episodes.each do |episode| %>
<tr>
  <td><%= link_to episode.castaway.name, episode %></td>
  <td><%= episode.episode_date %></td>
</tr>
<% end %>
</table>
