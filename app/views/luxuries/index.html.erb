<div class="row beach1">
  <% decades.values.each do |decade| %>
    <div class="col">
      <%= link_to decade, "?decade=#{decade}" %>
    </div>
  <% end %>
</div>
<div class = "row">

<div class ="col">
  <h2><%= image_tag("luxury.svg", :alt => "Luxury", :height => 32) %> Luxuries</h2>
<table class = "table table-striped table-sm">
<% @luxuries.each do |luxury| %>
<tr>
  <td><%= image_tag("luxury.svg", :alt => "Luxury", :height => 16) %> <%= link_to luxury.name, luxury %></td>
  <td><%= luxury.count %></td>
</tr>
<% end %>
</table>
</div>
</div>
